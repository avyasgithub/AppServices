# Replace the following URL with a public GitHub repo URL
$gitrepo="https://github.com/avyasgithub/AppServices.git"
$webappname="mywebapp7654"
$location="West Europe"
$resourcegroup ="AppServiceRG"
$serviceplan="WebAppForAppService20200219093402Plan"



# Create a web app.
New-AzWebApp -Name $webappname -Location $location -AppServicePlan $serviceplan -ResourceGroupName $resourcegroup

# Configure GitHub deployment from your GitHub repo and deploy once.
$PropertiesObject = @{
    repoUrl = "$gitrepo";
    branch = "master";
    isManualIntegration = "true";
}
Set-AzResource -PropertyObject $PropertiesObject -ResourceGroupName $resourcegroup -ResourceType Microsoft.Web/sites/sourcecontrols -ResourceName $webappname/web -ApiVersion 2015-08-01 -Force
